{
  "name": "zulip-mobile-notifs",
  "version": "0.6.2",
  "description": "Zulip notification bridge - polls Zulip API and delivers push notifications",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && pnpm build:shared",
    "build:shared": "node scripts/build-shared.js",
    "preview": "vite preview",
    "deploy": "npx wrangler deploy -c worker/wrangler.toml"
  },
  "keywords": [
    "zulip",
    "notifications",
    "push"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.2",
    "esbuild": "^0.24.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.0.0",
    "vue-tsc": "^3.1.5",
    "workbox-precaching": "^7.4.0"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/app": "^7.1.0",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/filesystem": "^7.1.4",
    "@capacitor/local-notifications": "^7.0.3",
    "@capacitor/preferences": "^7.0.2",
    "@capawesome/capacitor-background-task": "^7.0.1",
    "vue": "^3.5.25"
  }
}
